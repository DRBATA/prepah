{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "isolatedModules": true,
    "noEmit": false,
    "outDir": ".vercel_build_output"
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
